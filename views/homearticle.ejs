      <!-- <ul class=" z-depth-0 " style="border: none;"> -->
        <% if(posts.length==0){ %> 
            <h4 style="color: #623954; text-align: center;">No results Found</h4>
            <a href="/home/request/article"><h6 style=" text-decoration: underline; text-align: center;">You can request your Article here!!</h6></a>
            <hr>
            <%  } %> 
        <div class="row">
           
          <% posts.forEach(function(post){ %>
  
              <% var Id="h"+post.Id %> 
              <% var Idd="ht"+post.Id %> 
              <% var tags=[] %> 
              <% if(post.Title){ %> 
              <% tags=post.Title.split(",")} %> 
              
          <div class="col-12 mb-5">
             
              <div class="card CardArticle">
                  <div class="card-body">
                      <div class="row">
                          <div class="col-12">
                              <h3 class="card-title text-center"><%= post.Head %> </h3>
                          </div>
                      </div>
                      <hr>
                      <div class="row">
                          <div class="col-12" id=<%= Idd %> >
                              <div id=<%= Id %> class="d-none"><%= post.Blog %></div>
                          </div>
                          <a class="ArticleLink" class="text-center" href="/home/<%= post.Head %>-<%= post.Id %>  ">...Read more</a>
                          <!-- <div class="col-12">
                          </div> -->
                          <script>
                              var a="<%= Idd %>";
                              var b="<%= Id %>";
                              var txt=document.getElementById(b).textContent;
                              var s=txt.substring(0,1000);
                            //       for(let i=1000;i<s.length;i++){
                            //       if(s.charAt(i)!=='<' && s.charAt(i)!=='>' && s.charAt(i)!=='/' ){
                            //           s=s.substring(0,i);
                            //           break;
                            //       }
                            //   }
                              document.getElementById(a).innerHTML=s;
                          </script>
                      </div>
                      <hr>
                      <div class="row">
                        <div class="col-2 ">
                          <% tags.forEach(tag=>{ %>
                              
                            <a class="ArticleLink" href="/home/tag/<%= tag %>"> <%= tag %> </a>
                        
                        <% }) %>
                      </div>
                        </div>
                         
                          
                         
                         
                     
                     
                     
                    </div>
                </div>
                
          </div>
          <%})%> 
